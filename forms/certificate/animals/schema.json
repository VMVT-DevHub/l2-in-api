          "patvirtinimo-iregistravimo-nr": {
            "$ref": "#/definitions/patvirtinimo-iregistravimo-nr"
          },
          "identifikacijos-nr": {
            "title": "Identifikacijos Nr.",
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
