      "type": "string",
      "enum": ["Ausų įsagos", "Mikroschema", "Pasas", "Bandos numeris", "Tatuiruotė"]
    },

    "gyvuno-siuntos-paskirtis": {
      "type": "string",
      "enum": [
        "Veisti",
        "Skersti",
        "Produkcijai",
        "Tolesniam auginimui",
        "Gyvūno augintinio perkėlimas nekomerciniais tikslais"
      ]
