          "bandos-nr": {
            "title": "Bandos Nr.",
            "type": "string"
          },
          "pakuociu-skaicius": {
            "title": "Pakuočių skaičius",
            "type": "string"
          },
          "paukstides-nr": {
            "title": "Paukštidės Nr.",
            "type": "string"
