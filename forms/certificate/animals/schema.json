          "svoris": {
            "type": "number",
            "title": "Bendras svoris, kg"
          },
          "atpazinimo-priemone": {
            "$ref": "#/definitions/gyvuno-atpazinimo-priemone",
            "title": "Gyvūno atpažinimo priemonė"
