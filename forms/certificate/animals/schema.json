            "svoris": "Gyvūno svoris privalomas.",
            "atpazinimo-priemone": "Atpažinimo priemonė yra privaloma."
          }
        },

        "if": {
          "properties": {
            "siuntos-paskirtis": {
              "enum": ["Skersti"]
            }
          }
        },
        "then": {
          "required": ["asmuo"],
          "errorMessage": {
            "required": {
              "asmuo": "Gamintojas privalomas."
            }
          }
        },
        "properties": {
          "kodas": {
            "title": "Nomenklatūrinis pavadinimas ir kodas",
            "$ref": "#/definitions/gyvuno-kodas"
