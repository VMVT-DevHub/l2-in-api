          "rusis": {
            "type": "string",
            "title": "Gyvūnų rūšis"
          },
          "kiekis": {
            "type": "number",
            "title": "Gyvūnų skaičius, vnt"
          },
          "siuntos-paskirtis": {
            "title": "Siuntos paskirtis",
            "$ref": "#/definitions/gyvuno-siuntos-paskirtis"
